    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "env": {
                "GITHUB_TOKEN_LOGIN": "1",
                "GITHUB_TOKEN_PASSWORD": "2"
            },
            "args": [
                "-listen=0.0.0.0:8081",
                "-cacheDir=/tmp/test",
                "-proxy=https://goproxy.cn",
                "-proxy=https://mirrors.aliyun.com/goproxy/",
                "-exclude=github.com/iftechio",
                "-test"
            ]
        }
    ]
}
